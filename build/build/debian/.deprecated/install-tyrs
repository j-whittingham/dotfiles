#!/usr/local/bin/fish

log "... script installing tyrs"
install python-setuptools
install python-distutils-extra
install python-httplib2

cd /data/depot/twitter/tyrs
command rm -rf tyrs 2>/dev/null
git clone https://github.com/Nic0/tyrs.git

cd tyrs
sed -i 's/httplib2==0.6.0/httplib2>=0.6.0/' setup.py
python setup.py build
sudo python setup.py install


#!/usr/local/bin/fish
# beta elinks build

log "... script installing elinks"

cd /data/depot/www/elinks
rm -Rf elinks-0.13-20120926
tar xvfj elinks-current-0.13.tar.bz2

cd elinks-0.13-20120926
sed -i 's/CONFIG_256_COLORS=no/CONFIG_256_COLORS=yes/' features.conf
./configure
make
sudo make install


# ....................................................................... pikaur

[ -d /net/depot ] && cd /net/depot || cd /tmp
[ -d pikaur ] || git clone --depth 1 https://aur.archlinux.org/pikaur.git
cd pikaur
makepkg -fsri
pikaur -Sy


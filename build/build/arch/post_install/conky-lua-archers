BUILD_MOC=OFF\n
BUILD_MPD=ON\n
BUILD_NVIDIA=OFF\n
BUILD_OLD_CONFIG=OFF\n
DEFAULTNETDEV=eno1\n
makepkg -f\n
sudo pacman -U --noconfirm conky-lua-archers*pkg.tar.xz\n"


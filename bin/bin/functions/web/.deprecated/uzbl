# ...................................................................... Browser

if [ $1 ] ;then
  eval /usr/bin/uzbl-tabbed "$@"
else
  ping -c 1 -W 1 $SERVER >/dev/null 2>&1 && TTRSS=$SERVER || TTRSS=localhost
  eval /usr/bin/uzbl-tabbed http://$TTRSS:8000/tt-rss/
fi


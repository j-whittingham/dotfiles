# ${COLORS} defined in env

# ......................................................... Distribution install

# apply user specific configuration, see post_install
if [ -s $HOME/build/arch/user_install/$1 ] ;then
  arch && echo "${__GREEN}@user_install $1${NOCOLOR}"
  . $HOME/build/arch/user_install/$1
fi


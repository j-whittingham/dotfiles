# ....................................................................... Editor

# list current fkey assignments
fkeys() {
  cd ~/.vim/config
  for i in {1..12}
  do
    title "F$i"
    ack "<.*f$i>" *.vim
  done
  cd -
}

fkeys | less -RX

# vim: set ft=zsh: #

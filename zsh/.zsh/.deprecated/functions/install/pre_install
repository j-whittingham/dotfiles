# ${_colors_} defined in .zshenv

# .......................................................... Install preparation

# clean up environment for possible re-install of package
if [[ -s ~/build/archlinux/pre_install/$@ ]] ;then
  is_arch && echo "${_GREEN_}@ pre_install/$@${nocolor_}"
  ~/build/archlinux/pre_install/$@
fi

